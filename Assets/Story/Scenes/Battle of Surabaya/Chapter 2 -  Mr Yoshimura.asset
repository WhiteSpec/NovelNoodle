    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: Maafkan aku telat pulang,bu.
    audio: {fileID: 0}
    speaker: {fileID: 11400000, guid: 822fed3d72eaed148bf4ad7348b237aa, type: 2}
    actions:
